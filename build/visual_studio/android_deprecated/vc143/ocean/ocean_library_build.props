<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean Libraries</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_library_build_imported>True</ocean_library_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_common_imported)' != 'True'" Project="ocean_common.props"/>
  </ImportGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Static Debug|ARM64'">
    <OutDir>$(OceanRootPath)bin\lib\android_arm64_cl50_static_debug\</OutDir>
    <IntDir>$(OceanRootPath)bin\tmp\impl\ocean\$(ProjectName)\android_arm64_cl50_static_debug\</IntDir>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Static Debug|ARM'">
    <OutDir>$(OceanRootPath)bin\lib\android_arm_cl50_static_debug\</OutDir>
    <IntDir>$(OceanRootPath)bin\tmp\impl\ocean\$(ProjectName)\android_arm_cl50_static_debug\</IntDir>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Static Profile|ARM64'">
    <OutDir>$(OceanRootPath)bin\lib\android_arm64_cl50_static_profile\</OutDir>
    <IntDir>$(OceanRootPath)bin\tmp\impl\ocean\$(ProjectName)\android_arm64_cl50_static_profile\</IntDir>
