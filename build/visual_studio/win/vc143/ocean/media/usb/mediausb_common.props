<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean Media USB (Common)</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <ocean_mediausb_common_imported>True</ocean_mediausb_common_imported>
  </PropertyGroup>
  <ImportGroup Label="Dependencies">
	  <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\..\base\base_use.props"/>
    <Import Condition="'$(ocean_cv_use_imported)' != 'True'" Project="..\..\cv\cv_use.props"/>
	<Import Condition="'$(ocean_ioimage_use_imported)' != 'True'" Project="..\..\io\image\ioimage_use.props"/>
    <Import Condition="'$(ocean_media_use_imported)' != 'True'" Project="..\..\media\media_use.props"/>
    <Import Condition="'$(ocean_system_use_imported)' != 'True'" Project="..\..\system\system_use.props"/>
    <Import Condition="'$(ocean_systemusb_use_imported)' != 'True'" Project="..\..\system\usb\systemusb_use.props"/>
    <Import Condition="'$(ocean_systemusbvideo_use_imported)' != 'True'" Project="..\..\system\usb\video\systemusbvideo_use.props"/>
  </ImportGroup>
</Project>
