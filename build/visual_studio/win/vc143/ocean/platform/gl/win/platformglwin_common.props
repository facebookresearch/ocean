<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean Platform GL Win (Common)</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <ocean_platformglwin_common_imported>True</ocean_platformglwin_common_imported>
  </PropertyGroup>
  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\..\..\base\base_use.props"/>
    <Import Condition="'$(ocean_platform_use_imported)' != 'True'" Project="..\..\..\platform\platform_use.props"/>
    <Import Condition="'$(ocean_platformgl_use_imported)' != 'True'" Project="..\..\..\platform\gl\platformgl_use.props"/>
    <Import Condition="'$(ocean_platformwin_use_imported)' != 'True'" Project="..\..\..\platform\win\platformwin_use.props"/>
  </ImportGroup>
</Project>
