<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Tracking SLAM</_PropertySheetDisplayName>
  </PropertyGroup>
  
  <PropertyGroup>
    <ocean_trackingslam_use_imported>True</ocean_trackingslam_use_imported>
  </PropertyGroup>
  
  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_trackingslam_common_imported)' != 'True'" Project="trackingslam_common.props"/>
  </ImportGroup>
  
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <Link>
      <AdditionalDependencies>OceanTrackingSLAMD.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Static Profile' Or '$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Profile' Or '$(Configuration)'=='Shared Release'">
    <Link>
	    <AdditionalDependencies>OceanTrackingSLAM.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>
