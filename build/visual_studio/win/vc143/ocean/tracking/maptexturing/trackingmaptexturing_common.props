<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean Tracking MapTexturing (Common)</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <ocean_trackingmaptexturing_common_imported>True</ocean_trackingmaptexturing_common_imported>
  </PropertyGroup>
  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\..\base\base_use.props"/>
    <Import Condition="'$(ocean_cv_use_imported)' != 'True'" Project="..\..\cv\cv_use.props"/>
    <Import Condition="'$(ocean_math_use_imported)' != 'True'" Project="..\..\math\math_use.props"/>
    <Import Condition="'$(ocean_renderingglescenegraph_use_imported)' != 'True'" Project="..\..\rendering\glescenegraph\renderingglescenegraph_use.props"/>
    <Import Condition="'$(ocean_scenedescriptionsdxx3d_use_imported)' != 'True'" Project="..\..\scenedescription\sdx\x3d\scenedescriptionsdxx3d_use.props"/>
    <Import Condition="'$(ocean_tracking_use_imported)' != 'True'" Project="..\..\tracking\tracking_use.props"/>
  </ImportGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Debug|x64' Or '$(Configuration)|$(Platform)'=='Shared Debug|x64'">
    <Link>
      <AdditionalDependencies>ocean_devices_vrs_buckD.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(OCEAN_DEVELOPMENT_PATH)\3rdparty\ocean_devices_vrs_buck\lib\win_x64_vc_static_debug;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Profile|x64' Or '$(Configuration)|$(Platform)'=='Shared Profile|x64' Or '$(Configuration)|$(Platform)'=='Static Release|x64' Or '$(Configuration)|$(Platform)'=='Shared Release|x64'">
    <Link>
	    <AdditionalDependencies>ocean_devices_vrs_buck.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(OCEAN_DEVELOPMENT_PATH)\3rdparty\ocean_devices_vrs_buck\lib\win_x64_vc_static;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
</Project>
