    </ResourceCompile>
    <PostBuildEvent>
      <Message>Copying libraries to build directory</Message>
      <Command>if not exist "$(OCEAN_DEVELOPMENT_PATH)\bin\plugins\win_x86_vc143_shared_debug" mkdir "$(OCEAN_DEVELOPMENT_PATH)\bin\plugins\win_x86_vc143_shared_debug"
