<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean IO Image (Common)</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <ocean_ioimage_common_imported>True</ocean_ioimage_common_imported>
  </PropertyGroup>
  <ImportGroup Label="Dependencies">
	  <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\..\base\base_use.props"/>
    <Import Condition="'$(ocean_io_use_imported)' != 'True'" Project="..\..\io\io_use.props"/>
    <Import Condition="'$(ocean_mediawic_use_imported)' != 'True'" Project="..\..\media\wic\mediawic_use.props"/>
    <Import Condition="'$(ocean_mediaopenimagelibraries_use_imported)' != 'True'" Project="..\..\media\openimagelibraries\mediaopenimagelibraries_use.props"/>
    <Import Condition="'$(ocean_mediaspecial_use_imported)' != 'True'" Project="..\..\media\special\mediaspecial_use.props"/>
    <Import Condition="'$(ocean_system_use_imported)' != 'True'" Project="..\..\system\system_use.props"/>
  </ImportGroup>
</Project>
