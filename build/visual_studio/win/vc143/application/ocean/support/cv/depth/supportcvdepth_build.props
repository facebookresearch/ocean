<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean Support CV Depth</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_supportcvdepth_build_imported>True</ocean_supportcvdepth_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_supportcvdepth_common_imported)' != 'True'" Project="supportcvdepth_common.props"/>
    <Import Condition="'$(ocean_applicationsupportcv_build_imported)' != 'True'" Project="../application_support_cv_build.props"/>
  </ImportGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <TargetName>OceanApplicationSupportCVDepthD</TargetName>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Static Profile' Or '$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Profile' Or '$(Configuration)'=='Shared Release'">
    <TargetName>OceanApplicationSupportCVDepth</TargetName>
  </PropertyGroup>

  <ItemDefinitionGroup Condition="'$(Configuration)'=='Shared Debug' Or '$(Configuration)'=='Shared Profile' Or '$(Configuration)'=='Shared Release'">
    <ClCompile>
      <PreprocessorDefinitions>USE_OCEAN_APPLICATION_SUPPORT_CV_DEPTH_EXPORT;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>
