<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean Support Tracking PanoramaStitcher</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_supporttrackingpanoramastitcher_build_imported>True</ocean_supporttrackingpanoramastitcher_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_supporttrackingpanoramastitcher_common_imported)' != 'True'" Project="supporttrackingpanoramastitcher_common.props"/>
    <Import Condition="'$(ocean_applicationsupporttracking_build_imported)' != 'True'" Project="../application_support_tracking_build.props"/>
  </ImportGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <TargetName>OceanApplicationSupportTrackingPanoramaStitcherD</TargetName>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Static Profile' Or '$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Profile' Or '$(Configuration)'=='Shared Release'">
    <TargetName>OceanApplicationSupportTrackingPanoramaStitcher</TargetName>
  </PropertyGroup>

  <ItemDefinitionGroup Condition="'$(Configuration)'=='Shared Debug' Or '$(Configuration)'=='Shared Profile' Or '$(Configuration)'=='Shared Release'">
    <ClCompile>
      <PreprocessorDefinitions>USE_OCEAN_APPLICATION_SUPPORT_TRACKING_PANORAMASTITCHER_EXPORT;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>
