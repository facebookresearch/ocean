<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Application Ocean Demo IO Serialization Preview</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_applicationdemoioserializationpreview_build_imported>True</ocean_applicationdemoioserializationpreview_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_demoioserializationpreview_reference_imported)' != 'True'" Project="demoioserializationpreview_reference.props"/>
    <Import Condition="'$(ocean_applicationdemoio_build_imported)' != 'True'" Project="..\..\..\application_demo_io_build.props"/>
    <Import Condition="'$(ocean_application_window_build_imported)' != 'True'" Project="..\..\..\..\..\application_window_build.props" />
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\..\..\..\..\..\..\ocean\base\base_use.props" />
    <Import Condition="'$(ocean_cv_use_imported)' != 'True'" Project="..\..\..\..\..\..\..\ocean\cv\cv_use.props" />
    <Import Condition="'$(ocean_io_use_imported)' != 'True'" Project="..\..\..\..\..\..\..\ocean\io\io_use.props" />
    <Import Condition="'$(ocean_ioserialization_use_imported)' != 'True'" Project="..\..\..\..\..\..\..\ocean\io\serialization\ioserialization_use.props" />
    <Import Condition="'$(ocean_mediawic_use_imported)' != 'True'" Project="..\..\..\..\..\..\..\ocean\media\wic\mediawic_use.props" />
    <Import Condition="'$(ocean_platformwin_use_imported)' != 'True'" Project="..\..\..\..\..\..\..\ocean\platform\win\platformwin_use.props" />
  </ImportGroup>

  <ItemDefinitionGroup>
    <ResourceCompile>
      <AdditionalIncludeDirectories>$(OceanRootPath)impl\application\ocean\demo\io\serialization\preview\win;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ResourceCompile>
  </ItemDefinitionGroup>
</Project>
