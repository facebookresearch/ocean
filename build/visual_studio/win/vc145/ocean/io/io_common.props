<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean IO (Common)</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <ocean_io_common_imported>True</ocean_io_common_imported>
  </PropertyGroup>
  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\base\base_use.props"/>
    <Import Condition="'$(ocean_math_use_imported)' != 'True'" Project="..\math\math_use.props"/>
  </ImportGroup>
  <ItemDefinitionGroup>
	  <ClCompile>
      <AdditionalIncludeDirectories>$(OceanRootPath)3rdparty\openssl\1.0.2\h;$(OceanRootPath)3rdparty\tinyxml\2.0\h;$(OceanRootPath)3rdparty\zlib\h;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>libeay32.lib;tinyxml.lib;zlib.lib;Crypt32.lib;Wintrust.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\openssl\1.0.2\lib\$(Ocean3rdPartyConfigFolder);$(OceanRootPath)3rdparty\tinyxml\2.0\lib\$(Ocean3rdPartyConfigFolder);$(OceanRootPath)3rdparty\zlib\lib\$(Ocean3rdPartyConfigFolder);%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
</Project>
