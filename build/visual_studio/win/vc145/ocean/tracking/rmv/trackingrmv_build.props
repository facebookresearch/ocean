<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean Tracking RMV</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_trackingrmv_build_imported>True</ocean_trackingrmv_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_trackingrmv_common_imported)' != 'True'" Project="trackingrmv_common.props"/>
    <Import Condition="'$(ocean_trackingrmv_reference_imported)' != 'True'" Project="trackingrmv_reference.props"/>
    <Import Condition="'$(ocean_library_build_imported)' != 'True'" Project="..\..\ocean_library_build.props"/>
  </ImportGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <TargetName>OceanTrackingRMVD</TargetName>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Static Profile' Or '$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Profile' Or '$(Configuration)'=='Shared Release'">
    <TargetName>OceanTrackingRMV</TargetName>
  </PropertyGroup>

  <ItemDefinitionGroup Condition="'$(Configuration)'=='Shared Debug' Or '$(Configuration)'=='Shared Profile' Or '$(Configuration)'=='Shared Release'">
    <ClCompile>
      <PreprocessorDefinitions>USE_OCEAN_TRACKING_RMV_EXPORT;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>
