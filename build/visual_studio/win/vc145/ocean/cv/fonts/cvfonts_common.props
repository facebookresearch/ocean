<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean CV Fonts (Common)</_PropertySheetDisplayName>
  </PropertyGroup>
  <PropertyGroup>
    <ocean_cvfonts_common_imported>True</ocean_cvfonts_common_imported>
  </PropertyGroup>
  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\..\base\base_use.props"/>
    <Import Condition="'$(ocean_cv_use_imported)' != 'True'" Project="..\..\cv\cv_use.props"/>
    <Import Condition="'$(ocean_io_use_imported)' != 'True'" Project="..\..\io\io_use.props"/>
    <Import Condition="'$(ocean_math_use_imported)' != 'True'" Project="..\..\math\math_use.props"/>
  </ImportGroup>
  <ItemDefinitionGroup>
	  <ClCompile>
      <AdditionalIncludeDirectories>$(OceanRootPath)3rdparty\freetype\2.10\h;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Debug|x64' Or '$(Configuration)|$(Platform)'=='Shared Debug|x64'">
    <Link>
      <AdditionalDependencies>freetype.lib;libpng.lib;zlib.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\freetype\2.10\lib\win_x64_vc143_static_debug;$(OceanRootPath)3rdparty\libpng\lib\win_x64_vc143_static_debug;$(OceanRootPath)3rdparty\zlib\lib\win_x64_vc143_static_debug;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Debug|Win32' Or '$(Configuration)|$(Platform)'=='Shared Debug|Win32'">
    <Link>
      <AdditionalDependencies>freetype.lib;libpng.lib;zlib.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\freetype\2.10\lib\win_x86_vc143_static_debug;$(OceanRootPath)3rdparty\libpng\lib\win_x86_vc143_static_debug;$(OceanRootPath)3rdparty\zlib\lib\win_x86_vc143_static_debug;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Profile|x64' Or '$(Configuration)|$(Platform)'=='Shared Profile|x64' Or '$(Configuration)|$(Platform)'=='Static Release|x64' Or '$(Configuration)|$(Platform)'=='Shared Release|x64'">
    <Link>
	    <AdditionalDependencies>freetype.lib;libpng.lib;zlib.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\freetype\2.10\lib\win_x64_vc143_static;$(OceanRootPath)3rdparty\libpng\lib\win_x64_vc143_static;$(OceanRootPath)3rdparty\zlib\lib\win_x64_vc143_static;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Profile|Win32' Or '$(Configuration)|$(Platform)'=='Shared Profile|Win32' Or '$(Configuration)|$(Platform)'=='Static Release|Win32' Or '$(Configuration)|$(Platform)'=='Shared Release|Win32'">
    <Link>
	    <AdditionalDependencies>freetype.lib;libpng.lib;zlib.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\freetype\2.10\lib\win_x86_vc143_static;$(OceanRootPath)3rdparty\libpng\lib\win_x86_vc143_static;$(OceanRootPath)3rdparty\zlib\lib\win_x86_vc143_static;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
</Project>
