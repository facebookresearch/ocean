<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Application Test Media</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_applicationtestmedia_build_imported>True</ocean_applicationtestmedia_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_applicationtest_build_imported)' != 'True'" Project="..\application_test_build.props"/>
  </ImportGroup>

  <PropertyGroup>
    <OutDir>$(OCEAN_DEVELOPMENT_PATH)\bin\application\ocean\test\media\win\$(OceanConfigFolder)\</OutDir>
  </PropertyGroup>
</Project>
