<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Platform Android Application JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_platformandroidapplicationjnijava_use_imported>True</ocean_platformandroidapplicationjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_platformandroidapplicationjnijava_common_imported)' != 'True'" Project="platformandroidapplicationjnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanPlatformAndroidApplicationJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanPlatformAndroidApplicationJniD.aar</OceanPlatformAndroidApplicationJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanPlatformAndroidApplicationJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanPlatformAndroidApplicationJni.aar</OceanPlatformAndroidApplicationJniAAR>
  </PropertyGroup>
</Project>
