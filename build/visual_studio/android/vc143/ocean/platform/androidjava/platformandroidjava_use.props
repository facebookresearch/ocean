<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Platform Android Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_platformandroidjava_use_imported>True</ocean_platformandroidjava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_platformandroidjava_common_imported)' != 'True'" Project="platformandroidjava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanPlatformAndroidJavaAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanPlatformAndroidJavaD.aar</OceanPlatformAndroidJavaAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanPlatformAndroidJavaAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanPlatformAndroidJava.aar</OceanPlatformAndroidJavaAAR>
  </PropertyGroup>
</Project>
