<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean Platform Android Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_platformandroidjava_build_imported>True</ocean_platformandroidjava_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_java_library_build_imported)' != 'True'" Project="..\..\ocean_java_library_build.props"/>
    <Import Condition="'$(ocean_platformandroidjava_common_imported)' != 'True'" Project="platformandroidjava_common.props"/>
  </ImportGroup>

  <!-- Target name for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <TargetName>OceanPlatformAndroidJavaD</TargetName>
  </PropertyGroup>

  <!-- Target name for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <TargetName>OceanPlatformAndroidJava</TargetName>
  </PropertyGroup>

  <!-- Gradle dependencies (api scope, exported to consumers) -->
  <PropertyGroup>
    <GradleDependencies>com.google.android.gms:play-services-location:21.0.1</GradleDependencies>
  </PropertyGroup>
</Project>
