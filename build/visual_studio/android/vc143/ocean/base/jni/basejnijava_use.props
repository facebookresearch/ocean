<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Base JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_basejnijava_use_imported>True</ocean_basejnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_basejnijava_common_imported)' != 'True'" Project="basejnijava_common.props"/>
  </ImportGroup>

  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <GradleLocalAARDependencies>OceanBaseJniD.aar;$(GradleLocalAARDependencies)</GradleLocalAARDependencies>
  </PropertyGroup>

  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <GradleLocalAARDependencies>OceanBaseJni.aar;$(GradleLocalAARDependencies)</GradleLocalAARDependencies>
  </PropertyGroup>
</Project>
