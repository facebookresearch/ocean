<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean Tracking (Common)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_tracking_common_imported>True</ocean_tracking_common_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\base\base_use.props"/>
    <Import Condition="'$(ocean_cv_use_imported)' != 'True'" Project="..\cv\cv_use.props"/>
    <Import Condition="'$(ocean_cvadvanced_use_imported)' != 'True'" Project="..\cv\advanced\cvadvanced_use.props"/>
    <Import Condition="'$(ocean_cvdetector_use_imported)' != 'True'" Project="..\cv\detector\cvdetector_use.props"/>
    <Import Condition="'$(ocean_cvsegmentation_use_imported)' != 'True'" Project="..\cv\segmentation\cvsegmentation_use.props"/>
    <Import Condition="'$(ocean_geometry_use_imported)' != 'True'" Project="..\geometry\geometry_use.props"/>
    <Import Condition="'$(ocean_io_use_imported)' != 'True'" Project="..\io\io_use.props"/>
    <Import Condition="'$(ocean_math_use_imported)' != 'True'" Project="..\math\math_use.props"/>
    <Import Condition="'$(ocean_media_use_imported)' != 'True'" Project="..\media\media_use.props"/>
  </ImportGroup>
</Project>
