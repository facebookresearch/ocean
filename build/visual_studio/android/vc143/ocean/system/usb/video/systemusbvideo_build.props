<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean System USB Video (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_systemusbvideo_build_imported>True</ocean_systemusbvideo_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_systemusbvideo_common_imported)' != 'True'" Project="systemusbvideo_common.props"/>
    <Import Condition="'$(ocean_library_build_imported)' != 'True'" Project="..\..\..\ocean_library_build.props"/>
  </ImportGroup>

  <!-- Target name for Debug configurations -->
  <PropertyGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <TargetName>OceanSystemUSBVideoD</TargetName>
  </PropertyGroup>

  <!-- Target name for Release configurations -->
  <PropertyGroup Condition="'$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Release'">
    <TargetName>OceanSystemUSBVideo</TargetName>
  </PropertyGroup>
</Project>
