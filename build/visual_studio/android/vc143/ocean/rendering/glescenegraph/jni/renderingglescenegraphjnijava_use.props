<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Rendering GLESceneGraph JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_renderingglescenegraphjnijava_use_imported>True</ocean_renderingglescenegraphjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_renderingglescenegraphjnijava_common_imported)' != 'True'" Project="renderingglescenegraphjnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanRenderingGLESceneGraphJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanRenderingGLESceneGraphJniD.aar</OceanRenderingGLESceneGraphJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanRenderingGLESceneGraphJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanRenderingGLESceneGraphJni.aar</OceanRenderingGLESceneGraphJniAAR>
  </PropertyGroup>
</Project>
