<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean Rendering GLESceneGraph JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_renderingglescenegraphjnijava_build_imported>True</ocean_renderingglescenegraphjnijava_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_renderingglescenegraphjnijava_common_imported)' != 'True'" Project="renderingglescenegraphjnijava_common.props"/>
    <Import Condition="'$(ocean_java_library_build_imported)' != 'True'" Project="..\..\..\ocean_java_library_build.props"/>
  </ImportGroup>

  <!-- Target name for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <TargetName>OceanRenderingGLESceneGraphJniD</TargetName>
  </PropertyGroup>

  <!-- Target name for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <TargetName>OceanRenderingGLESceneGraphJni</TargetName>
  </PropertyGroup>
</Project>
