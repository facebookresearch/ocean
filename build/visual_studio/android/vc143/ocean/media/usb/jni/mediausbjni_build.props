<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean Media USB JNI (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_mediausbjni_build_imported>True</ocean_mediausbjni_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_mediausbjni_common_imported)' != 'True'" Project="mediausbjni_common.props"/>
    <Import Condition="'$(ocean_library_build_imported)' != 'True'" Project="..\..\..\ocean_library_build.props"/>
  </ImportGroup>

  <!-- Target name for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <TargetName>OceanMediaUSBJniD</TargetName>
  </PropertyGroup>

  <!-- Target name for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <TargetName>OceanMediaUSBJni</TargetName>
  </PropertyGroup>
</Project>
