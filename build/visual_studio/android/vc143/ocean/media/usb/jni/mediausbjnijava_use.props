<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Media USB JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_mediausbjnijava_use_imported>True</ocean_mediausbjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_mediausbjnijava_common_imported)' != 'True'" Project="mediausbjnijava_common.props"/>
  </ImportGroup>

  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <GradleLocalAARDependencies>OceanMediaUSBJniD.aar;$(GradleLocalAARDependencies)</GradleLocalAARDependencies>
  </PropertyGroup>

  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <GradleLocalAARDependencies>OceanMediaUSBJni.aar;$(GradleLocalAARDependencies)</GradleLocalAARDependencies>
  </PropertyGroup>
</Project>
