<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Media OpenImageLibraries JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_mediaopenimagelibrariesjnijava_use_imported>True</ocean_mediaopenimagelibrariesjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_mediaopenimagelibrariesjnijava_common_imported)' != 'True'" Project="mediaopenimagelibrariesjnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanMediaOpenImageLibrariesJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanMediaOpenImageLibrariesJniD.aar</OceanMediaOpenImageLibrariesJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanMediaOpenImageLibrariesJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanMediaOpenImageLibrariesJni.aar</OceanMediaOpenImageLibrariesJniAAR>
  </PropertyGroup>
</Project>
