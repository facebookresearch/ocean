<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Ocean Media OpenImageLibraries (Common)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_mediaopenimagelibraries_common_imported>True</ocean_mediaopenimagelibraries_common_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
	  <Import Condition="'$(ocean_common_imported)' != 'True'" Project="..\..\..\ocean_common.props"/>
    <Import Condition="'$(ocean_base_use_imported)' != 'True'" Project="..\..\base\base_use.props"/>
    <Import Condition="'$(ocean_cv_use_imported)' != 'True'" Project="..\..\cv\cv_use.props"/>
    <Import Condition="'$(ocean_media_use_imported)' != 'True'" Project="..\..\media\media_use.props"/>
    <Import Condition="'$(ocean_mediaspecial_use_imported)' != 'True'" Project="..\..\media\special\mediaspecial_use.props"/>
    <Import Condition="'$(ocean_system_use_imported)' != 'True'" Project="..\..\system\system_use.props"/>
  </ImportGroup>

  <ItemDefinitionGroup>
	  <ClCompile>
      <AdditionalIncludeDirectories>$(OceanRootPath)3rdparty\jpeglib\h;$(OceanRootPath)3rdparty\libpng\h;$(OceanRootPath)3rdparty\libtiff\h;$(OceanRootPath)3rdparty\libtiff\impl\h;$(OceanRootPath)3rdparty\libtiff\h\android;$(OceanRootPath)3rdparty\libtiff\impl\h\android;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>

  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Debug|ARM64' Or '$(Configuration)|$(Platform)'=='Shared Debug|ARM64'">
    <Link>
      <LibraryDependencies>libjpeg;png;libtiff;zlib;giflib;%(LibraryDependencies)</LibraryDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\jpeglib\lib\android_arm64_cl50_static_debug;$(OceanRootPath)3rdparty\libpng\lib\android_arm64_cl50_static_debug;$(OceanRootPath)3rdparty\libtiff\lib\android_arm64_cl50_static_debug;$(OceanRootPath)3rdparty\zlib\lib\android_arm64_cl50_static_debug;$(OceanRootPath)3rdparty\giflib\lib\android_arm64_cl50_static_debug;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Debug|ARM' Or '$(Configuration)|$(Platform)'=='Shared Debug|ARM'">
    <Link>
      <LibraryDependencies>libjpeg;png;libtiff;zlib;giflib;%(LibraryDependencies)</LibraryDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\jpeglib\lib\android_arm_cl50_static_debug;$(OceanRootPath)3rdparty\libpng\lib\android_arm_cl50_static_debug;$(OceanRootPath)3rdparty\libtiff\lib\android_arm_cl50_static_debug;$(OceanRootPath)3rdparty\zlib\lib\android_arm_cl50_static_debug;$(OceanRootPath)3rdparty\giflib\lib\android_arm_cl50_static_debug;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Profile|ARM64' Or '$(Configuration)|$(Platform)'=='Shared Profile|ARM64' Or '$(Configuration)|$(Platform)'=='Static Release|ARM64' Or '$(Configuration)|$(Platform)'=='Shared Release|ARM64'">
    <Link>
	    <LibraryDependencies>libjpeg;png;libtiff;zlib;giflib;%(LibraryDependencies)</LibraryDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\jpeglib\lib\android_arm64_cl50_static;$(OceanRootPath)3rdparty\libpng\lib\android_arm64_cl50_static;$(OceanRootPath)3rdparty\libtiff\lib\android_arm64_cl50_static;$(OceanRootPath)3rdparty\zlib\lib\android_arm64_cl50_static;$(OceanRootPath)3rdparty\giflib\lib\android_arm64_cl50_static;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Static Profile|ARM' Or '$(Configuration)|$(Platform)'=='Shared Profile|ARM' Or '$(Configuration)|$(Platform)'=='Static Release|ARM' Or '$(Configuration)|$(Platform)'=='Shared Release|ARM'">
    <Link>
	    <LibraryDependencies>libjpeg;png;libtiff;zlib;giflib;%(LibraryDependencies)</LibraryDependencies>
      <AdditionalLibraryDirectories>$(OceanRootPath)3rdparty\jpeglib\lib\android_arm_cl50_static;$(OceanRootPath)3rdparty\libpng\lib\android_arm_cl50_static;$(OceanRootPath)3rdparty\libtiff\lib\android_arm_cl50_static;$(OceanRootPath)3rdparty\zlib\lib\android_arm_cl50_static;$(OceanRootPath)3rdparty\giflib\lib\android_arm_cl50_static;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
</Project>
