<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Devices ARCore JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_devicesarcorejnijava_use_imported>True</ocean_devicesarcorejnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_devicesarcorejnijava_common_imported)' != 'True'" Project="devicesarcorejnijava_common.props"/>
  </ImportGroup>

  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <GradleLocalAARDependencies>OceanDevicesARCoreJniD.aar;$(GradleLocalAARDependencies)</GradleLocalAARDependencies>
  </PropertyGroup>

  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <GradleLocalAARDependencies>OceanDevicesARCoreJni.aar;$(GradleLocalAARDependencies)</GradleLocalAARDependencies>
  </PropertyGroup>
</Project>
