<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Devices Android JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_devicesandroidjnijava_use_imported>True</ocean_devicesandroidjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_devicesandroidjnijava_common_imported)' != 'True'" Project="devicesandroidjnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanDevicesAndroidJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanDevicesAndroidJniD.aar</OceanDevicesAndroidJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanDevicesAndroidJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanDevicesAndroidJni.aar</OceanDevicesAndroidJniAAR>
  </PropertyGroup>
</Project>
