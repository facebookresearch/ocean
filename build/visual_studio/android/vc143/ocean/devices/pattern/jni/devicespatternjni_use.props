<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Devices Pattern JNI (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_devicespatternjni_use_imported>True</ocean_devicespatternjni_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_devicespatternjni_common_imported)' != 'True'" Project="devicespatternjni_common.props"/>
  </ImportGroup>

  <!-- Link library for Debug configurations -->
  <PropertyGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <OceanDevicesPatternJniLib>OceanDevicesPatternJniD.lib</OceanDevicesPatternJniLib>
  </PropertyGroup>

  <!-- Link library for Release configurations -->
  <PropertyGroup Condition="'$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Release'">
    <OceanDevicesPatternJniLib>OceanDevicesPatternJni.lib</OceanDevicesPatternJniLib>
  </PropertyGroup>

  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>$(OceanDevicesPatternJniLib);%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>
