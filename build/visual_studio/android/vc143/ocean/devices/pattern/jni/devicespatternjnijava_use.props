<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Devices Pattern JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_devicespatternjnijava_use_imported>True</ocean_devicespatternjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_devicespatternjnijava_common_imported)' != 'True'" Project="devicespatternjnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanDevicesPatternJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanDevicesPatternJniD.aar</OceanDevicesPatternJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanDevicesPatternJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanDevicesPatternJni.aar</OceanDevicesPatternJniAAR>
  </PropertyGroup>
</Project>
