<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean Devices MapBuilding JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_devicesmapbuildingjnijava_use_imported>True</ocean_devicesmapbuildingjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_devicesmapbuildingjnijava_common_imported)' != 'True'" Project="devicesmapbuildingjnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanDevicesMapBuildingJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanDevicesMapBuildingJniD.aar</OceanDevicesMapBuildingJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanDevicesMapBuildingJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanDevicesMapBuildingJni.aar</OceanDevicesMapBuildingJniAAR>
  </PropertyGroup>
</Project>
