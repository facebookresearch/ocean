<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean SceneDescription SDX X3D JNI (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_scenedescriptionsdxx3djni_use_imported>True</ocean_scenedescriptionsdxx3djni_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_scenedescriptionsdxx3djni_common_imported)' != 'True'" Project="scenedescriptionsdxx3djni_common.props"/>
  </ImportGroup>

  <!-- Link against debug library -->
  <ItemDefinitionGroup Condition="$(Configuration.Contains('Debug'))">
    <Link>
      <AdditionalDependencies>OceanSceneDescriptionSDXX3DJniD;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>

  <!-- Link against release library -->
  <ItemDefinitionGroup Condition="!$(Configuration.Contains('Debug'))">
    <Link>
      <AdditionalDependencies>OceanSceneDescriptionSDXX3DJni;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>
