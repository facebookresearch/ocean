<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean SceneDescription SDX X3D (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_scenedescriptionsdxx3d_use_imported>True</ocean_scenedescriptionsdxx3d_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_scenedescriptionsdxx3d_common_imported)' != 'True'" Project="scenedescriptionsdxx3d_common.props"/>
  </ImportGroup>

  <!-- Link against debug library -->
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <Link>
      <AdditionalDependencies>OceanSceneDescriptionSDXX3DD;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>

  <!-- Link against release library -->
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Release'">
    <Link>
      <AdditionalDependencies>OceanSceneDescriptionSDXX3D;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>
