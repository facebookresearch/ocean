<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean SceneDescription SDX X3D JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_scenedescriptionsdxx3djnijava_use_imported>True</ocean_scenedescriptionsdxx3djnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_scenedescriptionsdxx3djnijava_common_imported)' != 'True'" Project="scenedescriptionsdxx3djnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanSceneDescriptionSDXX3DJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanSceneDescriptionSDXX3DJniD.aar</OceanSceneDescriptionSDXX3DJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanSceneDescriptionSDXX3DJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanSceneDescriptionSDXX3DJni.aar</OceanSceneDescriptionSDXX3DJniAAR>
  </PropertyGroup>
</Project>
