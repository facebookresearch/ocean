<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Use Ocean SceneDescription SDL OBJ JNI Java (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_scenedescriptionsdlobjjnijava_use_imported>True</ocean_scenedescriptionsdlobjjnijava_use_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_scenedescriptionsdlobjjnijava_common_imported)' != 'True'" Project="scenedescriptionsdlobjjnijava_common.props"/>
  </ImportGroup>

  <!-- AAR library path for Debug configurations -->
  <PropertyGroup Condition="$(Configuration.Contains('Debug'))">
    <OceanSceneDescriptionSDLOBJJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanSceneDescriptionSDLOBJJniD.aar</OceanSceneDescriptionSDLOBJJniAAR>
  </PropertyGroup>

  <!-- AAR library path for Release configurations -->
  <PropertyGroup Condition="!$(Configuration.Contains('Debug'))">
    <OceanSceneDescriptionSDLOBJJniAAR>$(OceanRootPath)bin\java\$(OceanJavaConfigFolder)\OceanSceneDescriptionSDLOBJJni.aar</OceanSceneDescriptionSDLOBJJniAAR>
  </PropertyGroup>
</Project>
