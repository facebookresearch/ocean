<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>Build Ocean SceneDescription SDL OBJ (Android)</_PropertySheetDisplayName>
  </PropertyGroup>

  <PropertyGroup>
    <ocean_scenedescriptionsdlobj_build_imported>True</ocean_scenedescriptionsdlobj_build_imported>
  </PropertyGroup>

  <ImportGroup Label="Dependencies">
    <Import Condition="'$(ocean_scenedescriptionsdlobj_common_imported)' != 'True'" Project="scenedescriptionsdlobj_common.props"/>
    <Import Condition="'$(ocean_library_build_imported)' != 'True'" Project="..\..\..\ocean_library_build.props"/>
  </ImportGroup>

  <!-- Target name for Debug configurations -->
  <PropertyGroup Condition="'$(Configuration)'=='Static Debug' Or '$(Configuration)'=='Shared Debug'">
    <TargetName>OceanSceneDescriptionSDLOBJD</TargetName>
  </PropertyGroup>

  <!-- Target name for Release configurations -->
  <PropertyGroup Condition="'$(Configuration)'=='Static Release' Or '$(Configuration)'=='Shared Release'">
    <TargetName>OceanSceneDescriptionSDLOBJ</TargetName>
  </PropertyGroup>
</Project>
